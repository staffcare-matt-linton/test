<Window x:Class="WpfApplication1.layouts.DockPanel1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1.layouts"
        mc:Ignorable="d"
        Title="DockPanel1" Height="300" Width="300">
    <DockPanel>
        <TextBox DockPanel.Dock="Top" Name="textBox" Width="100" HorizontalAlignment="Left"/>
        <Label DockPanel.Dock="Bottom" Name="label" Content="Label" Width="100" HorizontalAlignment="Left"/>
        <Button DockPanel.Dock="Left" Name="button" Content="Start" Margin="0,5,0,5"/>
        <Rectangle/>
    </DockPanel>
</Window>
