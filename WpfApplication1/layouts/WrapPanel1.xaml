<Window x:Class="WpfApplication1.layouts.WrapPanel1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1.layouts"
        mc:Ignorable="d"
        Title="WrapPanel1" Height="300" Width="300">
    <WrapPanel Orientation="Vertical">
        <TextBox Name="textBox" Width="100"/>
        <Button Name="button" Content="Start" Margin="0,5,0,5"/>
        <Label Name="label" Content="Label" />
    </WrapPanel>
</Window>
